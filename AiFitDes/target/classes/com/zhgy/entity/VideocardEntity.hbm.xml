<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zhgy.entity.VideocardEntity" table="videocard" schema="afds">
        <id name="id">
            <column name="id" sql-type="varchar(50)" length="50"/>
        </id>
        <property name="brands">
            <column name="brands" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="model">
            <column name="model" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="price">
            <column name="price" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="graphicsCardType">
            <column name="graphics_card_type" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="videoCardChip">
            <column name="video_card_chip" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="coreFrequency">
            <column name="core_frequency" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="memoryFrequency">
            <column name="memory_frequency" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="memoryCapacity">
            <column name="memory_capacity" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="memoryInterface">
            <column name="memory_interface" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="powerInterface">
            <column name="power_interface" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="powerSupplyMode">
            <column name="power_supply_mode" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="img">
            <column name="img" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>